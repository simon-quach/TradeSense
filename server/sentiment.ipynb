    "# takes in a dict of webpages from webscraping, returns sentiment analysis\n",
    "def utf8len(s):\n",
    "    return len(s.encode('utf-8'))\n",
    "\n",
    "def editScores(response):\n",
    "    scores=response['SentimentScore']\n",
    "    sum=scores['Positive']+scores['Negative']+scores['Mixed']\n",
    "    scores['Positive']/=sum\n",
    "    scores['Negative']/=sum\n",
    "    scores['Mixed']/=sum\n",
    "    scores['Neutral']=0\n",
    "    Keymax = max(zip(scores.values(), scores.keys()))[1]\n",
    "    return {\n",
    "        'Sentiment': Keymax,\n",
    "        'SentimentScore': scores\n",
    "    }\n",
    "\n",
    "\n",
    "def sentiment(webpage_list):\n",
    "    res = []\n",
    "    res_data = []\n",
    "    for webPage in webpage_list: \n",
    "        # uses sentiment analysis model to get metrics from specific webpage\n",
    "        if(utf8len(webPage['bodyText'])>10000):\n",
    "           continue\n",
    "            #webpage is too long to contain useful information and will skew data\n",
    "        if(utf8len(webPage['bodyText']) > 5000):  # cut bodyText to fit in function input\n",
    "            string_len=len(webPage['bodyText'])\n",
    "            string_len=string_len//2\n",
    "            s1=webPage['bodyText'][:string_len]\n",
    "            s2=webPage['bodyText'][(string_len+1):]\n",
    "            webPage['bodyText']=s1\n",
    "            newEntry={\n",
    "                'title': webPage['title'],\n",
    "                'bodyText': s2\n",
    "            }\n",
    "            webpage_list.append(newEntry)\n",
    "        response = client.detect_sentiment(\n",
    "            Text=webPage['bodyText'],\n",
    "            LanguageCode='en'\n",
    "        )\n",
    "        edited_response = editScores(response)\n",
    "        res.append(edited_response)\n",
    "        res_data.append(edited_response['Sentiment'])\n",
    "    overall_sentiment = (overall_sent(res_data)).lower().capitalize() # finds plurality of sentiment\n",
    "    scores=[]     \n",
    "    for dic in res:\n",
    "        scores.append(dic['SentimentScore'][overall_sentiment])\n",
    "    uncertainty = statistics.mean(scores)  # takes mean of uncertainty metric\n",
    "    return overall_sentiment, uncertainty\n",
    "\n",
    "\n",
    "def overall_sent(sentiment_markers):\n",
    "    c = Counter(sentiment_markers)\n",
    "    return c.most_common()[0][0]"
   ]
  },
